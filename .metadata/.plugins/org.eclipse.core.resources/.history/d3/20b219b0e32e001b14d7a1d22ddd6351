import java.util.Scanner;

public class SweetProgram {

	public static void main(String[] args) {
		System.out.println("Enter your name: ");
		Scanner scan = new Scanner(System.in);
		//String s = scan.nextLine();
		
		int i = 0;
		do {
			System.out.println("i is: "+i);
			i++;
		}while(false);
		
		
		
		scan.close();
	}

}
