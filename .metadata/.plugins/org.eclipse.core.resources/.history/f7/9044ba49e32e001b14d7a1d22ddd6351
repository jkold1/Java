import java.util.Scanner;

public class SweetProgram {

	public static void main(String[] args) {
		System.out.print("Enter your name: ");
		Scanner scan = new Scanner(System.in);
		//String s = scan.nextLine();
		
		
		
		int i = 0;
		while(i < 10) {
			System.out.println("i is: "+i);
			i++;
		}
		scan.close();
	}

}
